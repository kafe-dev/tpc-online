{% extends 'base.html.twig' %}

{% block title %}| TPC Admin{% endblock %}

{% block importmap %}{% endblock %}

{% block stylesheets %}
    <!-- App css -->
    <link href="{{ asset('admin/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" id="bootstrap-stylesheet" />
    <link href="{{ asset('admin/css/icons.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('admin/css/app.min.css') }}" rel="stylesheet" type="text/css" id="app-stylesheet" />
{% endblock %}

{% block body %}
    {% include '_partials/admin/topbar.html.twig' %}

    {% include '_partials/admin/left_sidebar.html.twig' %}

    <div class="content-page">
        <div class="content">
            <!-- Start container-fluid -->
            <div class="container-fluid">

                {% block main %}
                {% endblock %}

            </div>
            <!-- end container-fluid -->

            {% include '_partials/admin/footer.html.twig' %}
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    <!-- Vendor js -->
    <script src="{{ asset('admin/js/vendor.min.js') }}"></script>

    {% block js %}
        <script src="{{ asset('admin/libs/morris-js/morris.min.js') }}"></script>
        <script src="{{ asset('admin/libs/raphael/raphael.min.js') }}"></script>

        <script src="{{ asset('admin/js/pages/dashboard.init.js') }}"></script>
    {% endblock %}

    <!-- App js -->
    <script src="{{ asset('admin/js/app.min.js') }}"></script>
{% endblock %}